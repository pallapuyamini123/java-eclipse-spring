<!-- <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
   "http://www.springframework.org/dtd/spring-beans-2.0.dtd"> --> 
  <beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
<bean name="garden" class="ai.jobiak.source.Garden">
<property name="fruits">
<list>
<value>Mango</value>
<value>Kiwi</value>
<value>Watermelon</value>
<value>Orange</value>
<value>Muskmelon</value>


</list>

</property> 

<constructor-arg name="flowers">

<list>
<value>Jasmine</value>
<value>Cosmos</value>
<value>Lilly</value>
<value>Chisanthimam</value>
<value>Rose</value>
</list>
</constructor-arg> 
<property name="trees">
<set>

<value>Banyian Tree</value>
<value>The oak Tree</value>
<value>The Neem Tree</value>
<value>Tamrind Tree</value>
<value>Mango Tree</value>


</set>
</property>
<property name="cityFruit">
<map>
<entry key ="hyderabad" value ="Custored Apple"></entry>
<entry key ="kareemnagar" value ="MelonMusk"></entry>
<entry key ="Guntur" value ="Mirchi"></entry>
<entry key ="Rajamandry" value ="Mango"></entry>

</map>

</property>
</bean>
</beans>
